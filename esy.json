{
  "name": "gillian-platform",
  "version": "0.0.0",
  "private": true,
  "description": "Gillian Core and maintained instantiations",
  "author": {
    "name": "Verified Trustworthy Software Specification Group at Imperial College",
    "url": "https://vtss.doc.ic.ac.uk"
  },
  "homepage": "GillianDev.github.io",
  "license": "BSD-3-Clause",
  "esy": {
    "build": "dune build @install --promote-install-files",
    "install": [
      "esy-installer gillian.install"
    ],
    "buildsInSource": "_build"
  },
  "scripts": {
    "format-check": "dune build @fmt",
    "format": "dune build @fmt --auto-promote",
    "clean": "dune clean",
    "watch": "dune build @install --promote-install-files --watch",
    "odoc": "dune build @doc",
    "odoc:watch": "dune build @doc --watch"
  },
  "dependencies": {
    "@opam/batteries": "^3.0.0",
    "@opam/cmdliner": "^1.1.1",
    "@opam/dune": "^3.0",
    "@opam/fmt": "^0.8.8",
    "@opam/menhir": "20220210",
    "@opam/ppx_deriving_yojson": "*",
    "@opam/ppxlib": ">=0.18.0",
    "@opam/printbox": "0.5",
    "@opam/sqlite3": "5.0.2",
    "@opam/uuidm": "0.9.7",
    "@opam/visitors": ">=2.3",
    "@opam/yojson": "^1.7.0",
    "@opam/zarith": ">=1.12",
    "compcert": "giltho/CompCert#3a6f14016f0c9aaab2ddcdd61492e5fe54f12259",
    "ocaml": "4.14.x"
  },
  "devDependencies": {
    "ocaml": "4.14.x",
    "@opam/cmitomli": "*",
    "@opam/fileutils": "*",
    "@opam/utop": "*",
    "@opam/shexp": "v0.14.0",
    "@opam/ocaml-lsp-server": "1.12.2",
    "@opam/odoc": "^2.2.0",
    "@opam/ocamlformat": "0.24.1",
    "@opam/ocamlfind": "*",
    "concurrently": "^7.6.0"
  },
  "resolutions": {
    "@opam/conf-sqlite3":
      "ManasJayanth/esy-sqlite:esy.json#28dfc73a8eda790213a8359ad708231069ed7079"
  }
}